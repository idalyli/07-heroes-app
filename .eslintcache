                    {\n                        heroesFiltered.map(hero => (\n                            <HeroCard\n                                key={hero.id}\n                                {...hero}\n                            />\n                        ))\n                    }\n\n                </div>\n            </div>\n\n        </div>\n    )\n}\n","/home/idaly/Documents/Proyectos/UdemyReact/07-heroes-app/src/components/ui/Navbar.js",[],"/home/idaly/Documents/Proyectos/UdemyReact/07-heroes-app/src/components/heroes/HeroCard.js",[],"/home/idaly/Documents/Proyectos/UdemyReact/07-heroes-app/src/components/heroes/HeroList.js",[],"/home/idaly/Documents/Proyectos/UdemyReact/07-heroes-app/src/selectors/getHeroById.js",[],"/home/idaly/Documents/Proyectos/UdemyReact/07-heroes-app/src/selectors/getHeroesByName.js",[],"/home/idaly/Documents/Proyectos/UdemyReact/07-heroes-app/src/hooks/useForm.js",[],"/home/idaly/Documents/Proyectos/UdemyReact/07-heroes-app/src/selectors/getHeroesByPublisher.js",[],"/home/idaly/Documents/Proyectos/UdemyReact/07-heroes-app/src/data/heroes.js",[],"/home/idaly/Documents/Proyectos/UdemyReact/07-heroes-app/src/auth/AuthContext.js",[],"/home/idaly/Documents/Proyectos/UdemyReact/07-heroes-app/src/auth/authReducer.js",[],"/home/idaly/Documents/Proyectos/UdemyReact/07-heroes-app/src/types/types.js",[],{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","severity":1,"message":"95","line":25,"column":27,"nodeType":"96","messageId":"97","endLine":25,"endColumn":29},{"ruleId":"98","severity":1,"message":"99","line":56,"column":13,"nodeType":"100","endLine":60,"endColumn":15},{"ruleId":"101","severity":1,"message":"102","line":1,"column":26,"nodeType":"103","messageId":"104","endLine":1,"endColumn":34},"no-native-reassign",["105"],"no-negated-in-lhs",["106"],"no-empty-pattern","Unexpected empty object pattern.","ObjectPattern","unexpected","jsx-a11y/alt-text","img elements must have an alt prop, either with meanin